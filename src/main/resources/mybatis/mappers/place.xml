<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="user">
	
	<!-- 리스트 전체 가져오기 -->
	<select id="selectList" resultType="com.herero.vo.PlaceVo">
		<![CDATA[
			select 	place_no,
        			business_no,
					place_name,
					palce_address,
					place_content,
					place_image,
					place_category
			from place
			order by no desc
		]]>
	</select>

</mapper>